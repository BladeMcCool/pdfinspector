{
  "personal_info": {
    "name": "{candidate full name}",
    "email": "{candidate email}",
    "phone": "{candidate phone}",
    "linkedin": "{candidate linkedin url}",
    "location": "{candidate city and province/state}",
    "profile": "{candidate profile/brief desc}",
    "github": "{candidate github url}"
  },
  "skills": ["list","of","skills","..."],
  "work_history": [
    {
      "company": "{company name}",
      "tag": "{company tag}",
      "companydesc": "{company description}",
      "location": "{company location}",
      "jobtitle": "{company jobtitle}",
      "daterange": "{date range at the company}",
      "sortdate": "{sort date for display in resume work history}",
      "projects": [
        {
          "desc": "{ project description, description of what was done }",
          "sortdate": "{sort date for display in company project history}",
          "tech": "{comma separated list of technologies used on this project}",
          "github": "{github project link}",
          "location": "{location project was completed from}",
          "jobtitle": "{jobtitle while on project}"
        }
      ]
    }
  ],
  "education_v2": [
    {
      "institution": "{school name}",
      "location": "{location}}",
      "description": "{education description, diploma/degree type}",
      "graduated": "{date graduated}",
      "notes": [
        "relevant education notes (max one entry)"
      ]
    }
  ]
}